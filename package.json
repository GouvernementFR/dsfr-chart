{
  "name": "@gouvfr/dsfr-chart",
  "version": "2.0.3",
  "description": "Système de Design de l'État - Visualisation graphique",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GouvernementFR/dsfr-chart.git"
  },
  "author": "Service d'Information du Gouvernement <missak.keloglanian@pm.gouv.fr>",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist/*",
    "README.md",
    "CONTRIBUTING.md"
  ],
  "main": "./dist/DSFRChart/DSFRChart.umd.cjs",
  "module": "./dist/DSFRChart/DSFRChart.js",
  "exports": {
    ".": {
      "import": "./dist/bundler/main.js",
      "require": "./dist/bundler/main.cjs"
    },
    "./BarChart": {
      "import": "./dist/bundler/BarChart.js",
      "require": "./dist/bundler/BarChart.cjs"
    },
    "./BarLineChart": {
      "import": "./dist/bundler/BarLineChart.js",
      "require": "./dist/bundler/BarLineChart.cjs"
    },
    "./DataBox": {
      "import": "./dist/bundler/DataBox.js",
      "require": "./dist/bundler/DataBox.cjs"
    },
    "./GaugeChart": {
      "import": "./dist/bundler/GaugeChart.js",
      "require": "./dist/bundler/GaugeChart.cjs"
    },
    "./LineChart": {
      "import": "./dist/bundler/LineChart.js",
      "require": "./dist/bundler/LineChart.cjs"
    },
    "./MapChart": {
      "import": "./dist/bundler/MapChart.js",
      "require": "./dist/bundler/MapChart.cjs"
    },
    "./MapChartReg": {
      "import": "./dist/bundler/MapChartReg.js",
      "require": "./dist/bundler/MapChartReg.cjs"
    },
    "./PieChart": {
      "import": "./dist/bundler/PieChart.js",
      "require": "./dist/bundler/PieChart.cjs"
    },
    "./RadarChart": {
      "import": "./dist/bundler/RadarChart.js",
      "require": "./dist/bundler/RadarChart.cjs"
    },
    "./ScatterChart": {
      "import": "./dist/bundler/ScatterChart.js",
      "require": "./dist/bundler/ScatterChart.cjs"
    },
    "./TableChart": {
      "import": "./dist/bundler/TableChart.js",
      "require": "./dist/bundler/TableChart.cjs"
    },
    "./dist/DSFRChart/DSFRChart.js": "./dist/DSFRChart/DSFRChart.js",
    "./dist/DSFRChart/DSFRChart.umd.js": "./dist/DSFRChart/DSFRChart.umd.js",
    "./dist/DSFRChart/DSFRChart.css": "./dist/DSFRChart/DSFRChart.css"
  },
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "lint": "eslint --fix src/",
    "build:docs": "vite build",
    "build": "vite build --config=vite-components.config.js",
    "build-bar": "LIBRARY=BarChart vite build --config=vite-components.config.js",
    "build-bar-line": "LIBRARY=BarLineChart vite build --config=vite-components.config.js",
    "build-databox": "LIBRARY=DataBox vite build --config=vite-components.config.js",
    "build-gauge": "LIBRARY=GaugeChart vite build --config=vite-components.config.js",
    "build-line": "LIBRARY=LineChart vite build --config=vite-components.config.js",
    "build-map": "LIBRARY=MapChart vite build --config=vite-components.config.js",
    "build-map-reg": "LIBRARY=MapChartReg vite build --config=vite-components.config.js",
    "build-pie": "LIBRARY=PieChart vite build --config=vite-components.config.js",
    "build-radar": "LIBRARY=RadarChart vite build --config=vite-components.config.js",
    "build-scatter": "LIBRARY=ScatterChart vite build --config=vite-components.config.js",
    "build-table": "LIBRARY=TableChart vite build --config=vite-components.config.js",
    "build-bundler": "vite build --config=vite-bundler.config.js",
    "build:components": "concurrently npm:build npm:build-*",
    "storybook": "NODE_OPTIONS=--max_old_space_size=4096 storybook dev -p 6006",
    "build:storybook": "storybook build",
    "test": "playwright test",
    "test:update-snapshots": "playwright test --update-snapshots --reporter=line"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@gouvfr/dsfr": "^1.13.0",
    "@playwright/test": "^1.50.1",
    "@storybook/addon-essentials": "^8.5.5",
    "@storybook/blocks": "^8.5.5",
    "@storybook/test": "^8.5.5",
    "@storybook/web-components": "^8.5.5",
    "@storybook/web-components-vite": "^8.5.5",
    "@types/node": "^22.13.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "chart.js": "^4.4.7",
    "chroma-js": "^3.1.2",
    "concurrently": "^9.1.2",
    "d3-scale": "^4.0.2",
    "eslint": "^9.20.1",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.15.0",
    "html-to-image": "^1.11.12",
    "lit": "^3.2.1",
    "sass": "1.79.6",
    "standard": "^17.1.2",
    "storybook": "^8.5.5",
    "vite": "^6.1.0",
    "vue": "^3.5.13"
  }
}
